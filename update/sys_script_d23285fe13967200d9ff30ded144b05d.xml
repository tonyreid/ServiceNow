        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	// Add your code here
	
	var rc = current.rc_entry;
	var gr = new GlideRecord('cab_meeting');
	gr.addEncodedQuery('cab_definition=340320fe13167200d9ff30ded144b07d' );
	gr.addEncodedQuery('change_range_start<='+rc);
	gr.addEncodedQuery('change_range_end>='+rc);
	gr.query();
	while(gr.next()){
		var item = new GlideRecord('cab_agenda_item');
		item.insert();
		item.cab_meeting = gr.sys_id;
		item.task = current.sys_id;
		item.update();
	}
	
})(current, previous);]]></script>
